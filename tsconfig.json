{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "prisma"
  ],
  "include": [
    "src/**/*",
    "generated/**/*",
    "index.ts"
  ]
}